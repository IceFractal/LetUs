{
  "extends": [
    "airbnb-base",
    "plugin:jsx-a11y/recommended",
    "plugin:react/recommended"
  ],
  "plugins": [
    "jsx-a11y",
    "react"
  ],
  "globals": {
    "describe": true,
    "it": true,
    "document": true,
    "window": true,
    "io": true,
    "FB": true
  },
  "rules": {
    "react/prop-types": "off",
    "import/prefer-default-export": "off",
    "no-plusplus": "off",
    "no-multi-spaces": "off",
    "no-unused-vars": "warn"
  }
}
